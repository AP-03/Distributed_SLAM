<launch>

  <include file="$(find gazebo_ros)/launch/empty_world.launch"/>
  

  <param name="robot_description" textfile="$(find robot_description)/urdf/test_robot.urdf"/>
  

  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/>
  

  <node name="spawn_test_robot" pkg="gazebo_ros" type="spawn_model" 
        args="-urdf -model test_robot -param robot_description -z 0.05"/>
</launch>